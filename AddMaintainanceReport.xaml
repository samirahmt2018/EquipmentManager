<Window x:Class="Equipment_Manager.AddMaintainanceReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Equipment_Manager"
        mc:Ignorable="d"
        Title="Add Maintainance Report"  Height="472" Width="349" Icon="Images/flint.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="400"/>
            
            <RowDefinition Height="40"/>

        </Grid.RowDefinitions>
        <DataGrid Grid.Row="0" Name="maintainance_reports" AutoGeneratedColumns="maintainance_reports_AutoGeneratedColumns">
            
        </DataGrid>
        <Button Name="insertButton" Grid.Row="1" HorizontalAlignment="Right" Content="Apply" Width="81" Margin="0,5" Click="insertButton_Click"></Button>


    </Grid>
</Window>
